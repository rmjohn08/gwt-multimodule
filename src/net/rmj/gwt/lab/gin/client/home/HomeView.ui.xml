<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:Label>Enter name:</g:Label>
		<g:TextBox ui:field="textName"></g:TextBox>
		<g:Button styleName="{style.important}" ui:field="sendButton" text="Send" />
		<div>
			<g:Label ui:field="errorLabel"></g:Label>
		</div>
	
		<g:Label>Enter number:</g:Label>
		<g:IntegerBox ui:field="intBox"></g:IntegerBox>
		<g:Button styleName="{style.important}" ui:field="checkInteger" text="Send" />
		<div>
			<g:Label ui:field="errorLabelNumber"></g:Label>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 